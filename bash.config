source ~/.custombashrc
export PS1="\[\033[01;31m\]`hostname`[\w]\[\033[00m\]$ "
export XDG_CURRENT_DESKTOP=sway
export EDITOR='vim'
alias quit='exit'
alias get_idf='. $HOME/projects/esp-idf/export.sh'
alias kys='poweroff'
alias dc='docker-compose'
alias mousefix='sudo modprobe -r psmouse && sudo modprobe psmouse'
alias adbfix='adb reconnect && sleep 1 && adb reverse tcp:8081 tcp:8081'
